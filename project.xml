<?xml version="1.0" encoding="UTF-8"?>

<project>
  <pomVersion>3</pomVersion>
  <extend>${base.project.dir}/base-project.xml</extend>
  <name>Oracle Database support</name>
  <groupId>bundles</groupId>
  <artifactId>com.oracle.database</artifactId>
  <currentVersion>10.0</currentVersion>
  <organization/>
  <inceptionYear>2004</inceptionYear>
  <shortDescription>An idegaWeb bundle to support the Oracle database</shortDescription>
  <versions/>
  <branches/>
  <developers>
    <developer>
      <name>Tryggvi Larusson</name>
      <id>tryggvil</id>
      <email>tryggvi@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>bundles</groupId>
      <artifactId>com.idega.core</artifactId>
      <!--<version>1.9</version>-->
      <version>SNAPSHOT</version>
      <type>jar</type>
      <url>http://www.idega.com/</url>
      <properties>
        <eclipse.dependency>true</eclipse.dependency>
      </properties>
    </dependency>
    <dependency>
      <groupId>oracle_jdbc</groupId>
      <artifactId>oracle_jdbc</artifactId>
      <version>10.1.0.2.0/version>
      <type>jar</type>
      <url>http://www.oracle.com/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
  </build>
  <properties/>
</project>
